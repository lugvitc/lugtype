{
  "name": "monkeytype",
  "version": "1.14.3",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"./backend/**/*.ts\" \"./frontend/src/ts/**/*.ts\"",
    "install-all": "sh ./bin/install.sh",
    "install-windows": ".\\bin\\install.cmd",
    "docker": "cd backend && docker-compose up",
    "test-backend": "cd backend && yarn run test",
    "dev": "concurrently --kill-others \"yarn run dev-fe\" \"yarn run dev-be\"",
    "dev-be": "cd backend && yarn run dev",
    "dev-fe": "cd frontend && yarn run dev",
    "live": "concurrently --kill-others \"cd frontend && yarn run live\" \"cd backend && yarn run start\"",
    "audit-fe": "cd frontend && yarn run audit",
    "deploy-live": "cd frontend && yarn run deploy-live",
    "build-fe": "cd ./frontend && yarn run build-live",
    "pretty": "prettier --check './backend/**/*.ts' './frontend/src/**/*.{ts,scss}' './frontend/static/**/*.{json,html}'",
    "pr-check-lint-json": "cd frontend && npx gulp pr-check-lint-json",
    "pr-check-quote-json": "cd frontend && npx gulp pr-check-quote-json",
    "pr-check-language-json": "cd frontend && npx gulp pr-check-language-json",
    "pr-check-other-json": "cd frontend && npx gulp pr-check-other-json",
    "pr-check-ts": "cd frontend && npx gulp pr-check-ts",
    "pr-check-build-be": "cd backend && yarn run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "concurrently": "5.3.0",
    "eslint": "8.8.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-require-path-exists": "1.1.9",
    "husky": "4.3.0",
    "jsonschema": "1.4.0",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.0",
    "typescript": "4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn run lint"
    }
  },
  "packageManager": "yarn@3.2.1"
}
