{
  "name": "monkeytype",
  "version": "1.5.6",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "start:dev": "concurrently --kill-others \"cd ./frontend && npm run start:dev\" \"cd ./backend && npm run start:dev\"",
    "start:dev:be": "cd backend && npm run start:dev",
    "start:dev:fe": "cd frontend && npm run start:dev",
    "deploy:live": "cd frontend && npm run deploy:live",
    "install:all": "sh ./bin/install.sh",
    "lint": "./node_modules/.bin/eslint './backend/**/*.{js,ts}' './frontend/src/**/*.{js,ts}'",
    "pretty": "prettier --check './backend/**/*.{js,ts}' './frontend/src/**/*.{js,ts,scss}' './frontend/static/**/*.{json,html}'",
    "start:dev-ts": "concurrently --kill-others \"cd ./frontend && npm run start:dev-ts\" \"cd ./backend && npm run start:dev-ts\"",
    "start:dev:be-ts": "cd backend && npm run start:dev-ts",
    "start:dev:fe-ts": "cd frontend && npm run start:dev-ts",
    "deploy:live-ts": "cd frontend && npm run deploy:live-ts"
  },
  "engines": {
    "npm": "8.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "concurrently": "5.3.0",
    "eslint": "8.8.0",
    "eslint-config-standard": "16.0.3",
    "eslint-formatter-github": "1.0.11",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-require-path-exists": "1.1.9",
    "husky": "4.3.0",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "typescript": "^4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
